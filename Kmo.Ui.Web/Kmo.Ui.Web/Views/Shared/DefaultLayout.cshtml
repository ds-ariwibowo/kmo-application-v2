    <script>
        baseUrl = @Html.Raw(HttpUtility.JavaScriptStringEncode(new Uri(new Uri(this.Context.Request.Url.GetLeftPart(UriPartial.Authority)), Url.Content("~/")).ToString(), true));

        function ShowReportWindow(rptName, rptPars, title, selFormula) {
            var rptSession = {
                SessionID: '',
                ReportName: rptName,
                Parameters: rptPars,
                RecordSelectionFormula: selFormula,
                Title: title
            };

            App.startPageLoading({ animate: true });
            $.post('@Url.RouteUrl("Default", new { Controller = "Report", Action = "PrepareReport" })', rptSession).done(function (e) {
                if (e.success) {
                    window.open(e.data);
                }
                else {
                    bootbox.alert('Error From Server:<br />' + e.message);
                }
            });
            App.stopPageLoading();
        }

        var SessionTimeout = function () {
            var handlesessionTimeout = function () {
                $.sessionTimeout({
                    title: 'Session Timeout Notification',
                    message: 'Your session is about to expire.',
                    keepAliveUrl: '',
                    redirUrl:baseUrl + 'Account/Login',
                    logoutUrl:baseUrl + 'Account/Login',
                    warnAfter: 900000, //warn after 15 minutes (1 seconds : 1000)
                    redirAfter: 910000, //redirect after 10 secons,
                    ignoreUserActivity: true,
                    countdownMessage: 'Redirecting in {timer} seconds.',
                    countdownBar: true                   
                });
            }
            return {
                //main function to initiate the module
                init: function () {
                    handlesessionTimeout();
                }
            };
        }();

        $(document).ready(function () {
            SessionTimeout.init();
        });


    </script>
