    function setConditionFloor(value) {
        var idx = "";
        $('#floorCondition').text(value);
        $('#ddlCondition option').each(function (index, element) {
            //console.log(element.text + '-' + element.value);
            if (element.text == value) {
                idx = index;
                $('#ddlCondition').find('option[value=' + idx + ']').attr('selected', true);
                return false;
            }
        });
        if (idx === '') {
            $('#ddlCondition').find('option[value=6]').attr('selected', true);
            $('#c44').show();
            $('#c44').val(value);
            if (value === '-' || value === null || value === 'undefined') {
                $('#ddlCondition').find('option[value=0]').attr('selected', true);
                $('#c44').hide();
                $('#c44').val("");
            }
        }
    }

    function setLooSubject(value) {
        var idx = "";
        $('#looSubject').text(value);
        $('#ddlLooSubject option').each(function (index, element) {
            //console.log(element.text + '-' + element.value);
            if (element.text == value) {
                idx = index;
                $('#ddlLooSubject').find('option[value=' + idx + ']').attr('selected', true);
                return false;
            }
        });
        if (idx === '') {
            $('#ddlLooSubject').find('option[value=2]').attr('selected', true);
            $('#cDesc').show();
            $('#cDesc').val(value);
            if (value == null || value === 'undefined') {
                $('#ddlLooSubject').find('option[value=0]').attr('selected', true);
                $('#cDesc').hide();
                $('#cDesc').val("");
            }
        }
    }

    function pad (str, max) {
        str = str.toString();
        return str.length < max ? pad(str + " ", max) : str;
    }

